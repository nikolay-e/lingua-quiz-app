          "password": {
            "type": "string",
            "title": "Password"
          }
        },
        "type": "object",
        "required": ["username", "password"],
        "title": "UserLogin"
      },
      "UserProgressResponse": {
        "properties": {
          "vocabularyItemId": {
            "type": "string",
            "title": "Vocabularyitemid"
          },
          "level": {
            "type": "integer",
            "title": "Level"
          },
          "queuePosition": {
            "type": "integer",
            "title": "Queueposition"
          },
          "correctCount": {
            "type": "integer",
            "title": "Correctcount"
          },
          "incorrectCount": {
            "type": "integer",
            "title": "Incorrectcount"
          },
          "consecutiveCorrect": {
            "type": "integer",
            "title": "Consecutivecorrect"
          },
          "lastPracticed": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Lastpracticed"
          }
        },
        "type": "object",
        "required": [
          "vocabularyItemId",
          "level",
          "queuePosition",
          "correctCount",
          "incorrectCount",
          "consecutiveCorrect",
          "lastPracticed"
        ],
        "title": "UserProgressResponse"
      },
      "UserRegistration": {
        "properties": {
          "username": {
            "type": "string",
            "maxLength": 50,
            "minLength": 3,
            "title": "Username"
          },
          "password": {
            "type": "string",
            "maxLength": 128,
            "minLength": 8,
            "title": "Password"
          }
        },
        "type": "object",
        "required": ["username", "password"],
        "title": "UserRegistration"
      },
      "UserResponse": {
